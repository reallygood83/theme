{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_ENABLE_SHARING": "true",
    "NEXT_PUBLIC_ENABLE_COMMUNITY": "true", 
    "NEXT_PUBLIC_SHARING_DEBUG": "false"
  }
}